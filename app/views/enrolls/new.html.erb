<h1>New enroll</h1>

<%= semantic_form_for @enroll do |f| %>
  <%= f.input :course_id, :as => :check_boxes, :collection => Course.find(:all, :order => "id ASC"), :for => :Courses, :id => "Course", :include_blank => false %>
  <%= f.input :user_id, :as => :check_boxes, :collection => User.find(:all, :order => "id ASC"), :for => :users, :id => "User", :include_blank => false %>

 <%= f.action :submit, :as => :button %>

<% end %>



